#include <stdio.h>
int main (){
    int a=0;
    float b=0;
    double c=0;
    char d[1], e[50];
  
    printf("Insira um num inteiro : ");
    scanf("%i",&a);
    printf("Insira um num real : ");
    scanf("%f",&b);
    printf("Insira um num real maior : ");
    scanf("%lf",&c);
    printf("Insira a letra : ");
    scanf("%s",&d);
    printf("Insira a string : ");
    scanf("%s",&e);
    
    printf("O numero inteiro eh :%d\nO numero real eh :%f\nO numero real maior eh :%lf\nA letra e :%s\nA string e :%s",a,b,c,d,e);

}
